<head>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<br><br>
<% @stations.each do |station| %>
  <p><%= station.name + ". Created at: " + station.created_at.to_s %>
    <%= link_to "Update", "space_stations/#{station.id}/edit?stat=true" %>
    <%= link_to 'Delete', "space_stations/#{station.id}/", method: :delete %>
  </p>
  <br>
<% end %>
<%= link_to 'New Space Station', '/space_stations/new' %>
